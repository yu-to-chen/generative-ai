    "def generate_routine(policy):\n",
    "    try:\n",
    "        messages = [\n",
    "            {\n",
    "                \"role\": \"user\",\n",
    "                \"content\": f\"\"\"\n",
    "                    {CONVERSION_PROMPT}\n",
    "\n",
    "                    POLICY:\n",
    "                    {policy}\n",
    "                \"\"\"\n",
    "            }\n",
    "        ]\n",
    "\n",
    "        response = client.chat.completions.create(\n",
    "            model='o1-mini',\n",
    "            messages=messages\n",
    "        )\n",
    "        \n",
    "\n",
    "        return response.choices[0].message.content \n",
    "    except Exception as e:\n",
    "        print(f\"An error occurred: {e}\")"
   ]
