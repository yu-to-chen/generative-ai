{
  "name": "nemo-agent-toolkit-ui",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --names \"PROXY,UI\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:gateway\" \"npm run dev:next\"",
    "dev:gateway": "node proxy/server.js",
    "dev:next": "NEXT_TELEMETRY_DISABLED=1 next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --runInBand",
    "test:proxy": "jest __tests__/proxy --runInBand --testTimeout=15000"
  },
